{
  "name": "Distract Free",
  "version": "0.01",
  "manifest_version": 2,
  "description": "Display on new tab",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": { 
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "small_icon.png"
  },
  "permissions" : [
    "storage"
  ]
}
